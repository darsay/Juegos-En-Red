package es.urjc.JER.BattleWeenServer;

public class Cliente {
	private long id;
	private long time;
	
	Cliente(){
		time = 0;
	}
	
	public long getTime() {
		return time;
	}

	public void setTime(long time) {
		this.time = time;
	}

	

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}
	
	
}


