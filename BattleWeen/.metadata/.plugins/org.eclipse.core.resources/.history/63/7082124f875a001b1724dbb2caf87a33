package es.urjc.JER.BattleWeenServer;

public class Cliente {
	private long id;
	private float time;
	
	Cliente(){
		time = 0;
	}
	
	public float getTime() {
		return time;
	}

	public void setTime(float time) {
		this.time = time;
	}

	

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}
	
	
}


